        .top {
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            height: 900px;
        }

        .top::before {
            background: url("img/main.jpg") no-repeat center center/cover;
            content: "";
            position: absolute;
            top: 0;
            left: 0;
            width: 100%;
            height: 100%;
            z-index: -1;
            opacity: 0.25;
        }

        .home_logo {
            width: 324px;
            margin-bottom: 115px;
        }

        .home_bg {
            width: 600px;
        }

        .heading {
            font-weight: 600;
        }

        .middle {
            display: flex;
            flex-direction: column;
            justify-content: center;
            align-items: center;
            margin: 60px 0;
        }

        .content {
            margin: 40px 0;
            width: 60%;
        }

        .parallax {
            min-height: 400px;
            background: transparent;
            margin: 60px 0;
        }

        .mid-bottom {
            display: flex;
            flex-direction: row;
            margin: 30px 0;
        }

        .para {
            flex: 0.7;
            margin-left: 80px;
            font-size: larger;
        }

        .image {
            flex: 0.3;
        }

        .para-con {
            flex: 0.4;
            font-size: larger;
        }

        .image-con {
            margin-left: 80px;
            flex: 0.6;
        }

        .img-lock {
            width: 300px;
            margin: 20px 0;
        }

        .img-mobile {
            width: 90px;
        }

        .img-pc {
            width: 400px;
        }

        .btn {
